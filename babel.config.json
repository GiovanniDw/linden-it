{
    "env": {
        "development": {
            "sourceMaps": true,
            "plugins": ["source-map-support"]
        }
    },
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"edge": "17",
					"firefox": "60",
					"chrome": "67",
					"safari": "11.1"
				},
				"useBuiltIns": "usage"
			}
		]
    ]
    ,"plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": false,
        "displayName": false,
        "fileName": true,
        "minify": false,
        "transpileTemplateLiterals": false,
        "pure": true
      }
    ]
  ]
}
