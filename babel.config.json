{
	"env": {
		"development": {
			"sourceMaps": true,
			"plugins": [
        "source-map-support",
        [
	        "babel-plugin-styled-components",
          {
            "ssr": false,
            "fileName": false,
            "minify": false
          }
        ]
      ]
		}
	},
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"edge": "17",
					"firefox": "60",
					"chrome": "67",
					"safari": "11.1"
				},
				"useBuiltIns": "usage"
			}
		]
	]
}
